{
  "name": "social-media",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=social-media-showcase",
    "copy:readme": "copyfiles ./README.md ./projects/social-media",
    "build": "npm run build:lib && npm run build:demo",
    "build:all": "npm run build:lib && npm run build:element && npm run bundle:element",
    "build:lib": "npm run copy:readme && ng build social-media",
    "build:demo": "ng build social-media-showcase --base-href='https://vickui.github.io/social-media/'",
    "build:element": "ng build social-media && ng build --project=social-media-elements-helper",
    "bundle:element": "jscat ./dist/social-media-elements-helper/runtime.js ./dist/social-media-elements-helper/polyfills.js ./dist/social-media-elements-helper/vendor.js ./dist/social-media-elements-helper/styles.js ./dist/social-media-elements-helper/scripts.js ./dist/social-media-elements-helper/main.js > html-app/social-media.bundle.js",
    "format:check": "prettier --list-different 'projects/**/*.ts'",
    "format:write": "prettier --write 'projects/**/*.ts'",
    "format-package-json": "npm-run-all format-package-json:*",
    "format-package-json:lib": "prettier --write 'projects/social-media/package.json'",
    "format-package-json:main": "prettier --write 'package.json'",
    "test:lib": "ng test social-media",
    "test:lib-coverage": "ng test social-media --code-coverage --watch=false",
    "report-coverage:lib": "cat ./coverage/social-media/lcov.info | codecov",
    "publish:demo": "npx angular-cli-ghpages --dir=./dist/social-media-showcase",
    "publish:lib": "npx semantic-release",
    "adjust-version": "npm-run-all adjust-version:*",
    "adjust-version:lib": "rjp projects/social-media/package.json version $VERSION",
    "adjust-version:main": "rjp package.json version $VERSION",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/elements": "^8.2.14",
    "@angular/forms": "^8.0.3",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@webcomponents/custom-elements": "^1.4.1",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "angular-font-awesome": "^3.1.2",
    "document-register-element": "^1.14.3",
    "font-awesome": "~4.7.0",
    "g": "^2.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.13.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular-devkit/build-ng-packagr": "^0.800.6",
    "@angular/cli": "^8.0.6",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@commitlint/cli": "^7.6.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@commitlint/prompt": "^7.6.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/exec": "^3.3.8",
    "@semantic-release/git": "^7.0.18",
    "@types/jasmine": "^3.3.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codecov": "^3.7.0",
    "codelyzer": "^5.2.2",
    "copyfiles": "^2.3.0",
    "husky": "^2.7.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jscat": "0.0.2",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^8.2.1",
    "ng-packagr": "^5.7.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "protractor": "^5.4.4",
    "replace-json-property": "^1.4.3",
    "semantic-release": "^15.14.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vickui/social-media.git"
  }
}
